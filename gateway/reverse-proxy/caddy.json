                        }
                    ],
                    "tls_connection_policies": [
                        {
                            "match": {
                                "sni": [
                                    "*.home.lan"
                                ]
                            },
                            "certificate_selection": {
                                "any_tag": [
                                    "home-certificate"
                                ]
                            }
                        }
                    ]
                }
            }
        },
        "tls": {
            "certificates": {
                "load_files": [
                    {
                        "certificate": "/certs/home-certificate.pem",
                        "key": "/certs/home-private-key.pem",
                        "tags": [
                            "home-certificate"
                        ]
                    }
                ]
            },
            "automation": {
                "policies": [
                    {
                        "subjects": [
                            "prowlarr.home.lan",
                            "radarr.home.lan",
                            "sonarr.home.lan",
                            "torrent.home.lan",
                            "jellyseerr.home.lan",
                            "jellyfin.home.lan",
                            "ntfy.${DUCKDNS_SUBDOMAIN}.duckdns.org",
                            "jellyseerr.${DUCKDNS_SUBDOMAIN}.duckdns.org",
                            "jellyfin.${DUCKDNS_SUBDOMAIN}.duckdns.org",
                            "nextcloud.${DUCKDNS_SUBDOMAIN}.duckdns.org",
                            "owntracks.${DUCKDNS_SUBDOMAIN}.duckdns.org"
                        ]
                    },
                    {
                        "subjects": [
                            "*.home.lan"
                        ],
                        "issuers": [
                            {
                                "module": "acme",
                                "trusted_roots_pem_files": [
                                    "/certs/home-ca-certificate.pem"
                                ]
                            },
                            {
                                "module": "zerossl",
                                "trusted_roots_pem_files": [
                                    "/certs/home-ca-certificate.pem"
                                ]
                            }
                        ]
                    },
                    {
                        "subjects": [
                            "*.${DUCKDNS_SUBDOMAIN}.duckdns.org"
                        ],
                        "issuers": [
                            {
                                "challenges": {
                                    "dns": {
                                        "provider": {
                                            "api_token": "${DUCKDNS_TOKEN}",
                                            "name": "duckdns"
                                        }
                                    }
                                },
                                "module": "acme"
                            },
                            {
                                "challenges": {
                                    "dns": {
                                        "provider": {
                                            "api_token": "${DUCKDNS_TOKEN}",
                                            "name": "duckdns"
                                        }
                                    }
                                },
                                "module": "zerossl"
                            }
                        ]
                    }
                ]
            }
        }
    }
}
